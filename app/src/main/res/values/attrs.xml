<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="FlikerProgressBar">
        <attr name="textSize" format="dimension"/>
        
        <attr name="loadingColor" format="color"/>

        <attr name="stopColor" format="color"/>

        <attr name="radius" format="dimension"/>
        
        <attr name="borderWidth" format="dimension"/>

    </declare-styleable>

    <declare-styleable name="SnowingView">
        <attr format="reference" name="src"/>
    </declare-styleable>

    <declare-styleable name="CircleRelativeLayoutLayout">
        <attr name="background_color" format="color" />
        <attr name="background_alpha" format="integer" />
    </declare-styleable>
</resources>